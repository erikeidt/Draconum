<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netcoreapp2.1</TargetFramework>
		<AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
		<GenerateSerializationAssemblies>Off</GenerateSerializationAssemblies>
		<OutputPath></OutputPath>
		<OutputType>Exe</OutputType>
		<RootNamespace>com.erikeidt.Draconum</RootNamespace>
		<StartupObject>com.erikeidt.Draconum.ExpressionParserTest.Program</StartupObject>
		<AssemblyName>Draconum Expression Parser</AssemblyName>
	</PropertyGroup>

	<ItemGroup>
		<Compile Include="..\1. Token Scanner\Token Scanner Library\**\*.cs">
			<Link>Token Scanner Library-Linked\%(RecursiveDir)%(FileName)</Link>
		</Compile>
	</ItemGroup>

</Project>
